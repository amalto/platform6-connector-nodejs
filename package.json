        "@types/mem": "^1.1.2",
        "@types/node": "^8.10.2",
        "@types/uuid": "^3.4.3",
        "ava": "^5.1.0",
        "nyc": "^15.1.0",
        "ts-loader": "^3.5.0",
        "ts-node": "^5.0.1",
